{
  "name": "WaniPlus",
  "version": "1.0.3",
  "manifest_version": 3,
  "key": "",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "alarms",
    "unlimitedStorage",
    "scripting"
  ],
  "host_permissions": [
    "https://waniplus.com/*",
    "https://www.wanikani.com/*"
  ],
  "optional_permissions": [],
  "optional_host_permissions": [],
  "background": {
    "service_worker": "src/background.js"
  },
  "externally_connectable": {
    "matches": [
      "https://www.wanikani.com/*",
      "https://waniplus.com/*"
    ]
  },
  "action": {
    "default_popup": "index.html"
  }
}