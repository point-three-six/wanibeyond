{
  "name": "WaniPlus",
  "version": "0.0.1",
  "manifest_version": 3,
  "key": "",
  "permissions": [
    "storage",
    "alarms",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "https://waniplus.com/*",
    "http://localhost:3000/*"
  ],
  "optional_permissions": [],
  "optional_host_permissions": [],
  "background": {
    "service_worker": "src/background.js"
  },
  "externally_connectable": {
    "matches": [
      "https://www.wanikani.com/*",
      "https://www.waniplus.com/*",
      "http://localhost:3000/*"
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "src/utils/context.js",
        "src/utils/api.js",
        "src/utils/interceptor.js",
        "src/utils/notify.js",
        "src/pages/pg-wk-home.js",
        "src/pages/pg-wk-lessons.js",
        "src/pages/pg-wk-reviews.js"
      ],
      "matches": [
        "https://www.wanikani.com/*"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.wanikani.com/*"
      ],
      "js": [
        "src/wanikani.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}